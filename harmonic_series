/****************************************************
* Вычисление первых count членов гармонического ряда.
*****************************************************/
var
    integer i, count, d;
    float res;
begin
    read count;
    i := 0;
    d := 1;
    while i < count do
        res := 1 / d;
        write res;
        i := i + 1;
        d := d + 1;
    end;
end