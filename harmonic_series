/****************************************************
* Вычисление первых count членов гармонического ряда.
*****************************************************/
var
    integer div, i, count;
    float r, res;
begin
    read count; /*reading*/
    i := 0;
    div := 1;
    while i < count do
        res := 1 / div;
        write res;
        div := div + 1;
        i := i + 1;
    end;
end